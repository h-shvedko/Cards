<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.cards.shvedko.Controller.RemoveCardController">
    <children>
        <GridPane alignment="CENTER" prefHeight="300.0" prefWidth="400.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="129.0" minHeight="0.0" prefHeight="26.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="267.0" minHeight="10.0" prefHeight="120.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="267.0" minHeight="10.0" prefHeight="73.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="267.0" minHeight="10.0" prefHeight="25.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="183.0" minHeight="10.0" prefHeight="62.0" vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="183.0" minHeight="10.0" prefHeight="28.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <GridPane prefHeight="87.0" prefWidth="400.0" GridPane.rowIndex="4">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <Button fx:id="close" mnemonicParsing="false" onAction="#handleCloseButton" prefHeight="63.0"
                                prefWidth="149.0" text="Нет" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                            <font>
                                <Font size="20.0"/>
                            </font>
                        </Button>
                        <Button fx:id="agree" layoutX="10.0" layoutY="16.0" mnemonicParsing="false"
                                onAction="#handleAgreeButton" prefHeight="63.0" prefWidth="149.0" text="Да"
                                GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                            <font>
                                <Font size="20.0"/>
                            </font>
                        </Button>
                    </children>
                </GridPane>
                <Label fx:id="messageRemove" alignment="CENTER" contentDisplay="CENTER" prefHeight="99.0" prefWidth="589.0"
                       textAlignment="CENTER" wrapText="true"
                       GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
                    <font>
                        <Font size="31.0"/>
                    </font>
                </Label>
                <Label fx:id="cardName" alignment="CENTER" contentDisplay="CENTER" prefHeight="35.0" prefWidth="400.0"
                       text="Label" GridPane.rowIndex="2">
                    <font>
                        <Font size="30.0"/>
                    </font>
                </Label>
            </children>
        </GridPane>
    </children>
</AnchorPane>
