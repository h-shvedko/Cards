<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.*?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.cards.shvedko.Controller.AuthenticationController">
    <children>
        <GridPane prefHeight="400.0" prefWidth="600.0" AnchorPane.topAnchor="0.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="144.0" minWidth="10.0" prefWidth="97.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="475.0" minWidth="10.0" prefWidth="409.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="379.0" minWidth="10.0" prefWidth="89.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label alignment="BOTTOM_LEFT" prefHeight="83.0" prefWidth="418.0" text="LOGIN" GridPane.columnIndex="1"
                       GridPane.rowIndex="1"/>
                <Label alignment="BOTTOM_LEFT" layoutX="109.0" layoutY="10.0" prefHeight="83.0" prefWidth="418.0"
                       text="PASSWORD" GridPane.columnIndex="1" GridPane.rowIndex="3"/>
                <GridPane GridPane.columnIndex="1" GridPane.rowIndex="6">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="104.0" text="Cancel"
                                GridPane.columnIndex="1" GridPane.halignment="CENTER"
                                onAction="#handleCancelButtonAction"/>
                        <Button layoutX="216.0" layoutY="10.0" mnemonicParsing="false" prefHeight="55.0"
                                prefWidth="101.0" text="Login" GridPane.halignment="CENTER"
                                onAction="#handleSubmitButtonAction" fx:id="closeApp"/>
                    </children>
                </GridPane>
                <TextField prefHeight="42.0" prefWidth="408.0" GridPane.columnIndex="1" GridPane.rowIndex="2"
                           fx:id="login"/>
                <Label alignment="CENTER" prefHeight="37.0" prefWidth="411.0" text="Cards learning application"
                       GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                    <font>
                        <Font size="32.0"/>
                    </font>
                </Label>
                <PasswordField prefHeight="39.0" prefWidth="411.0" GridPane.columnIndex="1" GridPane.rowIndex="4"
                               fx:id="password"/>
                <Hyperlink alignment="TOP_CENTER" prefHeight="53.0" prefWidth="113.0" text="Create new account"
                           GridPane.columnIndex="1" GridPane.rowIndex="5" fx:id="createAccount" onAction="#createAccount"/>
            </children>
        </GridPane>
    </children>
</AnchorPane>
